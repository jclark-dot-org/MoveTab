{
  "name": "MoveTab",
  "description": "A chrome extension that helps you move your tabs from one window to another without dragging and dropping.",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": { 
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png" },
  "browser_action": {
    "default_title": "Move this tab",
    "default_icon": {
        "19": "icon19in.png",
	"38": "icon38in.png"
    }
  },
  "manifest_version": 2
}